{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Java Functions",
            "type": "java",
            "request": "attach",
            "hostName": "127.0.0.1",
            "port": 5005,
            "preLaunchTask": "func: host start"
        },
        {
            "type": "java",
            "name": "Spring Boot-AzureBatchFunctionApplication<azure-batch-function>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.azure_batch_function.AzureBatchFunctionApplication",
            "projectName": "azure-batch-function",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}